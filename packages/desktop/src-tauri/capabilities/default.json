{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "dialog:default",
    "dialog:allow-save",
    "fs:default",
    "fs:allow-write-text-file",
    "fs:allow-write-file",
    "fs:allow-temp-write",
    "opener:default",
    "opener:allow-open-url",
    {
      "identifier": "opener:allow-open-path",
      "allow": [{ "path": "$TEMP/*" }]
    },
    "updater:default",
    "process:allow-restart"
  ]
}
